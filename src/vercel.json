{
    "version": 2,
    "name": "blogs platform",
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "src/index.ts"
        },
        {
            "src": "/(.*)",
            "status": 404,
            "headers": {
                "Content-Type": "application/json"
            },
            "continue": true
        }
    ]
}